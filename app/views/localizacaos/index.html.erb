<h1>Listing localizacaos</h1>

<table>
  <tr>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @localizacaos.each do |localizacao| %>
  <tr>
    <td><%= localizacao.descricao %></td>
    <td><%= link_to 'Show', localizacao %></td>
    <td><%= link_to 'Edit', edit_localizacao_path(localizacao) %></td>
    <td><%= link_to 'Destroy', localizacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Localizacao', new_localizacao_path %>
